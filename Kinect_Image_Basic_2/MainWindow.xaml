<Window x:Class="Kinect_Image_Basic_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="700" Width="700" Loaded="Window_Loaded" Closing="Window_Closing">
    <Grid>
        <Label HorizontalAlignment="Left" Name="Message" Loaded="Message_Loaded">TEST</Label>
        <Label HorizontalAlignment="Right" Name="Address"></Label>
        <Image Name="Image" Width="640" Height="640" MouseMove="Image_MouseMove" />
    </Grid>
</Window>
